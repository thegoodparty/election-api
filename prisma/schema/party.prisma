model Party {
  id          Int           @id @default(autoincrement())
  databaseId  Int?          @map("database_id")
  createdAt   DateTime      @map("created_at")
  updatedAt   DateTime      @map("updated_at")

  name        String
  shortName   String        @map("short_name")

  // Many-to-many with OfficeHolder through officeHolderParty join table
  officeHolderParties OfficeHolderParty[]

  // Many-to-many with Candidacy through candidacyParty join table
  candidacyParties CandidacyParty[]
  
  @@map("party")
} 