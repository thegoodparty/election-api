/**
 * Join table for the many-to-many relationship between candidacy and party
 */
model CandidacyParty {
  id          Int       @id @default(autoincrement())
  
  candidacyId Int       @map("candidacy_id")
  candidacy   Candidacy @relation(fields: [candidacyId], references: [id])
  
  partyId     Int       @map("party_id")
  party       Party     @relation(fields: [partyId], references: [id])
  
  @@unique([candidacyId, partyId])
  @@map("candidacy_party")
} 