model Race {
  id                                String        @id @db.Uuid
  brHashId                          String?
  brDatabaseId                      Int?
  positionLevel                     PositionLevel
  electionDate                      DateTime
  positionSlug                      String
  // Data
  normalizedPositionName            String
  positionDescription               String
  // City / Municipality Facts
  municipalityPopluation            Int
  municipalityDensity               Float
  municipalityIncomeHouseholdMedian Int
  municipalityUnemploymentRate      Float
  municipalityHomeValue             Int
  // County Facts
  cityLargest                       String
  countyPopulation                  Int
  countyDensity                     Float
  countyIncomeHouseholdMedian       Int
  countyUnemploymentRate            Float
  countyHomeValue                   Int
  state                             String // Two letter abbreviation
  countyName                        String
  municipalityName                  String
  Candidacy                         Candidacy[]
}
