model Race {
  id                                String        @id @db.Uuid
  brHashId                          String?
  brDatabaseId                      Int?
  positionLevel                     PositionLevel
  electionDate                      DateTime
  electionDay                       String
  positionSlug                      String
  // Position
  normalizedPositionName            String
  positionDescription               String
  filingOfficeAddress               String
  filingPhoneNumber                 String
  paperworkInstructions             String
  filingRequirements                String
  isRunoff                          Boolean
  isPrimary                         Boolean
  partisanType                      String
  locationName                      String
  filingDateStart                   String
  filingDateEnd                     String
  employmentType                    String
  eligibilityRequirements           String
  salary                            String
  // City / Municipality Facts
  municipalityPopluation            Int
  municipalityDensity               Float
  municipalityIncomeHouseholdMedian Int
  municipalityUnemploymentRate      Float
  municipalityHomeValue             Int
  // County Facts
  cityLargest                       String
  countyPopulation                  Int
  countyDensity                     Float
  countyIncomeHouseholdMedian       Int
  countyUnemploymentRate            Float
  countyHomeValue                   Int
  state                             String // Two letter abbreviation
  countyName                        String
  municipalityName                  String
  // PositionElection
  frequency                         Int[]
  Candidacy                         Candidacy[]
}
